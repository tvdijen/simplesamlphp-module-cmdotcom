{
    "name": "tvdijen/simplesamlphp-module-cmdotcom",
    "description": "SMS as Second Factor module",
    "type": "simplesamlphp-module",
    "keywords": ["simplesamlphp", "cm", "cmcom", "cmdotcom", "sms"],
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Tim van Dijen",
            "email": "tvdijen@gmail.com"
        }
    ],
    "config": {
        "preferred-install": {
            "simplesamlphp/simplesamlphp": "source",
            "*": "dist"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "simplesamlphp/composer-module-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "SimpleSAML\\Module\\cmdotcom\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSAML\\Test\\Utils\\": "vendor/simplesamlphp/simplesamlphp/tests/Utils"
        }
    },
    "require": {
        "php": ">=7.4 || ^8.0",
        "giggsey/libphonenumber-for-php": "^8.12.43",
        "guzzlehttp/guzzle": "^7.4",
        "simplesamlphp/assert": "^0.2.13",
        "simplesamlphp/composer-module-installer": "^1.1.8",
        "simplesamlphp/simplesamlphp": "^2.0.0-beta.9"
    },
    "require-dev": {
        "simplesamlphp/simplesamlphp-test-framework": "^1.1.6"
    },
    "support": {
        "issues": "https://github.com/tvdijen/simplesamlphp-module-cmdotcom/issues",
        "source": "https://github.com/tvdijen/simplesamlphp-module-cmdotcom"
    }
}
